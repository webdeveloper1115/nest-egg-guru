{
  "name": "nest-egg-guru",
  "description": "Nest Egg Guru Website",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "pretest": "npm install",
    "postinstall": "bower install",
    "deploy:production": "ansible-playbook -i deploy/production deploy/deploy.yml --ask-sudo-pass --extra-vars='target=nesteggguru' load_balanced=false",
    "deploy:production-loadbalanced": "ansible-playbook -i deploy/production deploy/deploy.yml --ask-sudo-pass --extra-vars='target=nesteggguru load_balanced=true' ",
    "deploy:staging-master": "ansible-playbook -i deploy/staging deploy/deploy.yml --ask-sudo-pass --extra-vars='target=nesteggguru load_balanced=false'",
    "deploy:staging-bobby-ko": "ansible-playbook -i deploy/staging deploy/deploy.yml --ask-sudo-pass --extra-vars='target=nesteggguru load_balanced=false deployment_branch=bobby-ko'"
  },
